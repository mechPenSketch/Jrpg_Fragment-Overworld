; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="JRPG Fragment - Overworld (Dialogic)"
run/main_scene="res://overworld/maps/Outdoors.tscn"
config/features=PackedStringArray("4.0")
config/icon="res://icon.png"

[autoload]

DialogicSingleton="*res://addons/dialogic/Other/DialogicSingleton.gd"
GlobalOverworld="*res://overworld/global.gd"

[display]

window/stretch/mode="2d"
window/size/height=576

[editor_plugins]

enabled=PackedStringArray()

[layer_names]

2d_physics/layer_1="Wall"

[locale]

translations=PackedStringArray("res://translations/dialogs.en.translation", "res://translations/dialogs.es.translation", "res://translations/dialogs.ja.translation")

[rendering]

renderer/rendering_method="gl_compatibility"
environment/default_environment="res://default_env.tres"
